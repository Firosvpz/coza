					<div class="p-r-50 p-t-5 p-lr-0-lg">
						<h4 class="mtext-105 cl2 js-name-detail p-b-14">
							<%=products.name%>
						</h4>


						<% if (products.offer) { var discount=(products.price * products.offer.percentage /
							100).toFixed(0); } else { var discount=0; } %>

							<% if (categories.length> 0) { %>
								<% categories.forEach(category=> { %>
									<% if (category.offer && category.categoryName===products.category) {
										discount=(products.price * category.offer.percentage / 100).toFixed(0); } %>
										<% }) %>
											<% } %>

												<% if (discount> 0) { %>
													<span class="mb-0 stext-105 cl3 "> <del
															class="text-danger text-decoration-line-through">₹
															<%= products.price %>
														</del>
														<span class="price stext-105 cl3 ">₹ <%= products.price -
																discount %>
																
														</span></span>
													<% } else { %>
														<span class="stext-105 cl3">
															₹<%= products.price %>
														</span>
														<% } %>


															<p class="stext-102 cl3 p-t-23">
																Description : <%=products.description%>
															</p>

															<!--  -->


															<div class="">
																<div class="size-204 flex-w flex-m respon6-next">
																	<input type="text" name="productId" id="productId"
																		value="<%= products._id %>" hidden>
																</div>
																<div class="w-100"></div>
																<div class="input-group col-md-2  mb-3 ">
																	<% if(products.quantity> 0){ %>

