        <%
        function getStatusText(status) {
            switch (status) {
                case 'cancelled':
                    return 'Cancelled';
                case 'request_cancellation':
                    return 'Cancellation Requested';
                case 'request_return':
                    return 'Return Requested';
                case 'returned':
                    return 'Returned';
                case 'delivered':
                    return 'Delivered';
                case 'shipped':
                    return 'Shipped'; // Add a case for "shipped"
                case 'placed':
                    return 'Placed'; // Add a case for "placed"
                default:
                    console.log('Unknown Status:', status);
                    return 'Unknown';
            }
        }
        %>
        
        <%
        function getProgressBarWidth(status) {
            let width;
            switch (status) {
                case 'cancelled':
                    width = '25%';
                    break;
                case 'request_cancellation':
                case 'request_return':
                    width = '25%';
                    break;
                case 'returned':
                    width = '75%';
                    break;
                case 'delivered':
                    width = '100%';
                    break;
                case 'out_for_delivery':
                case 'shipped':
                    width = '50%';
                    break;
                case 'placed':
                    width = '25%';
                    break;
                default:
                    width = '0%';
                    console.log('Unknown Status:', status);
                    break;
            }
            console.log('Status:', status, 'Width:', width);
            return width;
        }
        %>
        
                <div class="container w-75 rounded">

                    <% if (order && order.items && order.items.length> 0) { %>
                        <% order.items.forEach((item, index)=> { %>
                            <div class="order-details p-4">

                                <div class="d-flex flex-wrap">

                                    <img src="/webimages/<%= item.product_id.images[1] %>"
                                        class="img-fluid me-2 mb-2 rounded-5" width="20%" height="20%"
                                        alt="image not display">

                                    <div>
                                        <p class="display-4 fw-bold text-center text-success">
                                            <%= item.product_id.name%>
                                        </p>
                                        <table class="table ">
                                            <tr>
                                                <th>Total</th>
                                                <td class="column-3">
                                                    <% if (item.product_id.offer) { 
                                                        var discount = (item.product_id.price * item.product_id.offer.percentage / 100).toFixed(0); 
                                                    } else { 
                                                        var discount = 0; 
                                                    } %>
                                                    
                                                    <% if (categories.length > 0) { %>
                                                        <% categories.forEach(category => { %>
                                                            <% if (category.offer && category.categoryName === item.product_id.category) { 
                                                                discount = (item.product_id.price * category.offer.percentage / 100).toFixed(0); 
                                                            } %>
                                                        <% }); %>
                                                    <% } %>
                                            
                                                    <% if (discount > 0) { %>
                                                        <span class="mb-0 stext-105 cl3">
                                                            <del class="text-danger text-decoration-line-through">₹<%= item.product_id.price*item.quantity %></del>
                                                            <span class="price stext-105 cl3">₹<%= item.product_id.price* item.quantity - discount %></span>
                                                           
                                                        </span>
                                                    <% } else { %>
                                                        <span class="stext-105 cl3">₹<%= item.product_id.price* item.quantity %></span>
                                                    <% } %>
                                                </td>
                                            </tr>
                                            <tr>
                                                <th>Quantity</th>
                                                <td><%= item.quantity%></td>
                                            </tr>
                                            <tr>
                                                <th>Delivery Address</th>
                                                <td><%= order.delivery_address %></td>
                                            </tr>
                                            <tr>
                                                <th>Order Date</th>
                                                <td><%= moment(order.date).format('DD/MM/YYYY') %></td>
                                            </tr>
                                            <tr>
                                                <th>Payment Method</th>
                                                <td><%= order.payment %></td>
                                            </tr>
                                            <tr>
                                                <th>Expected Delivery</th>
                                                <td><%= order.expected_delivery %></td>
                                            </tr>
                                        </table>
                                        
                                    </div>

                                </div>

                                <!-- Now you can use getStatusText and getProgressBarWidth in your template -->
                                <div class="progress mt-3 ">
                                    <div class="progress-bar bg-danger" role="progressbar" 
                                        style="width: <%= getProgressBarWidth(item.ordered_status) %>; text-align: end;">
                                        <%= getProgressBarWidth(item.ordered_status) %>
                                    </div>
                                </div>
                                

                                <!-- Display status under the progress bar -->
                                <div class="mt-2 text-end text-danger">
                                    <p><strong>Status:</strong>
                                        <%= getStatusText(item.ordered_status) %>
                                    </p>
                                </div>
                                

                                <div class="item-actions mt-2">
                                    <% if (!['request_cancellation', 'cancelled' , 'request_return' , 'returned'
                                        ].includes(item.ordered_status)) { %>
                                        <% if (item.ordered_status==='delivered' ) { %>
                                            <% const currentDate=new Date(); %>
                                                <% const returnPolicyDays=7; %>

                                                    <!-- Calculate the return expiry date based on the current date -->
                                                    <% const returnExpiryDate=new Date(order.date); %>
                                                        <% console.log('gunda', returnExpiryDate) %>
                                                            <% returnExpiryDate.setDate(returnExpiryDate.getDate() +
                                                                returnPolicyDays); %>

                                                                <% if (currentDate <=returnExpiryDate) { %>
                                                                    <button
                                                                        class="cancel-btn return-btn btn btn-outline-danger"
                                                                        onclick="returnOrder('<%= order._id %>','<%= item._id %>',event)">Return
                                                                        Item</button>
                                                                    <p class="text-muted mt-3" id="return-text">Please
                                                                        note
                                                                        that
                                                                        order returns are only possible within <%=
                                                                            returnPolicyDays %> days from the delivery
                                                                            date.
                                                                    </p>
                                                                    <% } else { %>
                                                                        <p class="text-muted mt-3">Return expired</p>
                                                                        <% } %>

                                                                            <% } else { %>
                                                                                <button
                                                                                    class="cancel-btn btn btn-outline-danger"
                                                                                    onclick="cancelOrder('<%= order._id %>','<%= item._id %>',event)">Cancel
                                                                                    Item</button>
                                                                                <% } %>
                                                                                    <% } %>
                                </div>

                            </div>
                            <% }) %>
                                <% } else { %>
                                    <div class="no-items">No items found in the order.</div>
                                    <!-- If the order or items are null or empty, show a message or handle as needed -->
                                    <% } %>
                </div>



